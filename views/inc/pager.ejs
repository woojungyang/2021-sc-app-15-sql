<link rel="stylesheet" href="/css/pager.css">
<div class="pager-wrapper">
	<ul class="pager-wrap">
		<li class="pager pager-first">
			<a href="/book/1" class="material-icons">first_page</a>
		</li>
		<li class="pager pager-prev-set">
			<a href="/book/<%= prevPager %>" class="material-icons">keyboard_double_arrow_left</a>
		</li>
		<li class="pager pager-prev">
			<a href="/book/<%= prevPage %>" class="material-icons">chevron_left</a>
		</li>
		<% for(let i=startPage; i<=endPage; i++) { %>
			<li class="pager <%= page === i ? 'active' : '' %>">
				<a href="/book/<%= i %>"><%= i %></a>
			</li>
		<% } %>
		<li class="pager pager-next">
			<a href="/book/<%= nextPage %>" class="material-icons">chevron_right</a>
		</li>
		<li class="pager pager-next-set">
			<a href="/book/<%= nextPager %>" class="material-icons">keyboard_double_arrow_right</a>
		</li>
		<li class="pager pager-last">
			<a href="/book/<%= totalPage %>" class="material-icons">last_page</a>
		</li>
	</ul>
</div>